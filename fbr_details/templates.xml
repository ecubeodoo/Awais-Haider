                                <field name="secp_regis_no"/>
                            </group>
                            <group>
                                <field name="secp_email"/>
                                <field name="secp_registered_address"/>
                                <field name="secp_address_1"/>
                                <field name="secp_address_2"/>
                            </group>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</openerp>